<?xml version="1.0" encoding="utf-8"?>
<pattern>
  <l-system>
    <axiom>GG</axiom>
    <rules>
      <rule>G=F[+FG]F[-FG]F[&gt;FG]</rule>
    </rules>
    <iterations>3</iterations>
  </l-system>
  <model-param>
    <initAngle>0</initAngle>
    <rotAngle>60</rotAngle>
    <devAngle>10</devAngle>
    <nrSides>8</nrSides>
    <segmentLength>5</segmentLength>
    <cutLengthFactor>0,9</cutLengthFactor>
    <segmentThickness>1</segmentThickness>
    <cutThicknessFactor>0,9</cutThicknessFactor>
  </model-param>
</pattern>